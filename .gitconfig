[remote "scrapy"]
    url = https://github.com/scrapy/scrapy.git
    fetch = +refs/heads/*:refs/remotes/scrapy/*
[remote "origin"]
    url = git@github.com:huangyingw/scrapy.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = scrapy
    target = scrapy/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/tools/scrapy/scrapy
